#alias {ar} {#con mud.arctic.org 2700} {default}
#alias {assist} {od assist $leader;ass} {default}
#alias {autoassist} { #if $AutoAssist  {#var AutoAssist 0;#showme {colorcodes} {&WAutoassist: &gOFF&w};updstatus} {#var AutoAssist 1;#showme {colorcodes} {&WAutoassist: &RON&w};updstatus} } {default}
#alias {bb} {bash %0;stand} {default}
#alias {clrbuffs} {#var buffrow { };#status $statusbuffsbar {}} {default}
#alias {clrstatusbars} { #status 1; #status 2; #status 3; #status 4; #status 5; } {default}
